<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<APIProxy revision="14" name="oauth-mock-v1">
    <Basepaths>/oauth</Basepaths>
    <ConfigurationVersion majorVersion="4" minorVersion="0"/>
    <CreatedAt>1513019477532</CreatedAt>
    <CreatedBy>kurtkanaskie@google.com</CreatedBy>
    <!-- @description gets replaced with SCM details, see pom.xml -->
    <Description>OAuth proxy to generate access token for client_credentials and password grant_types using Mock to authenticate a demo user. Uses Mock tokens as attribute on Edge token.</Description>
    <DisplayName>oauth-mock-v1</DisplayName>
    <LastModifiedAt>1513020148764</LastModifiedAt>
    <LastModifiedBy>kurtkanaskie@google.com</LastModifiedBy>
    <Policies>
        <Policy>AM-RemoveSensitiveStuff</Policy>
        <Policy>AM-SetUsernamePasswordFromRequest</Policy>
        <Policy>AM-SetUsernamePasswordFromToken</Policy>
        <Policy>EV-TokenParams</Policy>
        <Policy>JS-AdjustExpiryValues</Policy>
        <Policy>KV-config-values</Policy>
        <Policy>OA-GenerateAccessToken-ClientCredentials</Policy>
        <Policy>OA-GenerateAccessToken-Implicit</Policy>
        <Policy>OA-GenerateAccessToken-Password</Policy>
        <Policy>OA-GetRefreshTokenInfo</Policy>
        <Policy>OA-RefreshAccessToken</Policy>
        <Policy>OA-VerifyAPIKey-and-Secret</Policy>
        <Policy>RF-AutenticateFailed</Policy>
        <Policy>RF-path-suffix-not-found</Policy>
        <Policy>SC-AuthenticateClient</Policy>
        <Policy>SC-AuthenticateUser</Policy>
    </Policies>
    <ProxyEndpoints>
        <ProxyEndpoint>default</ProxyEndpoint>
    </ProxyEndpoints>
    <Resources>
        <Resource>jsc://JS-AdjustExpiryValues.js</Resource>
    </Resources>
    <Spec></Spec>
    <TargetServers/>
    <TargetEndpoints/>
    <validate>false</validate>
</APIProxy>
